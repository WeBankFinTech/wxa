{
    "presets": [
        [
            "@babel/preset-env",
            {
                "targets": {
                    "node": "14"
                },
                "exclude": ["proposal-dynamic-import"]
            }
        ]
    ],
    "plugins": [
        ["@babel/plugin-transform-runtime", {"corejs": false, "version": "7.8.7"}],
        "@babel/plugin-proposal-class-properties"
    ]
}
