{
  "name": "@wxa/cli2",
  "version": "2.1.5",
  "description": "cli for wxa development",
  "main": "dist/wxa.js",
  "scripts": {
    "build": "npm run build:src && npm run build:lib && npm run build:polyfill",
    "build:src": "babel src --out-dir dist",
    "build:lib": "babel lib --out-dir lib-dist",
    "build:polyfill": "sh scripts/buildLib.sh",
    "dev:src": "babel --watch src --out-dir dist --source-maps",
    "dev": "npm run build:polyfill & npm run dev:src & npm run dev:lib",
    "dev:lib": "babel --watch lib --out-dir lib-dist",
    "prepublishOnly": "sh scripts/prepublish.sh",
    "travis-dev": "babel src --out-dir dist"
  },
  "author": "iveswen",
  "license": "MIT",
  "bin": {
    "wxa2": "bin/wxa.js"
  },
  "repository": "https://github.com/wxajs/wxa.git",
  "homepage": "https://wxajs.github.io/wxa/",
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-transform-async-to-generator": "^7.0.0",
    "@babel/polyfill": "^7.0.0",
    "babel-eslint": "^9.0.0",
    "eslint": "^4.15.0",
    "eslint-config-google": "^0.9.1",
    "semver": "^5.5.0",
    "webpack": "^4.28.3",
    "webpack-cli": "^3.2.0"
  },
  "dependencies": {
    "@babel/generator": "^7.1.3",
    "@babel/parser": "^7.1.3",
    "@babel/plugin-proposal-class-properties": "^7.2.3",
    "@babel/plugin-proposal-decorators": "^7.2.3",
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/preset-env": "^7.2.3",
    "@babel/runtime": "^7.2.0",
    "@babel/template": "^7.1.2",
    "@babel/traverse": "^7.1.4",
    "@babel/types": "^7.1.6",
    "@wxa/compiler-babel": "latest",
    "@wxa/compiler-sass": "latest",
    "chalk": "^2.3.0",
    "chokidar": "^2.0.0",
    "commander": "^2.14.1",
    "core-js": "^3.0.0-beta.6",
    "debug": "^3.1.0",
    "deepmerge": "^2.2.1",
    "dom-serializer": "^0.1.1",
    "ejs": "^2.6.2",
    "find-cache-dir": "^1.0.0",
    "find-root": "^1.1.0",
    "globby": "^8.0.1",
    "htmlparser2": "^3.10.1",
    "inquirer": "^6.2.0",
    "js-base64": "^2.4.0",
    "lodash": "^4.17.15",
    "mkdirp": "^0.5.1",
    "node-notifier": "^5.2.1",
    "promise.prototype.finally": "^3.1.0",
    "shelljs": "^0.8.3",
    "tapable": "^1.0.0",
    "valid-url": "^1.0.9",
    "xmldom": "^0.1.27"
  },
  "publishConfig": {
    "access": "public"
  }
}
