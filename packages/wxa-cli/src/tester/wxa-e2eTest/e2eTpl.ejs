/* eslint-disable no-undef */
const automator = require('miniprogram-automator');
const path = require('path');
let miniProgram;

async function run() {
    setTimeout(async () => {
        miniProgram = await automator.launch({
            cliPath: '<%= cliPath %>',
            projectPath: path.resolve(process.cwd(), './dist/'),
        });
    
        await miniProgram.currentPage();
    }, 15000);
};

run();
