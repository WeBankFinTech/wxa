{
    "extends": ["tslint:recommended", "tslint-config-airbnb"],
    "rules": {
        "no-bitwise": false,
        "no-empty-interface":false,
        "max-line-length": false,
        "align": false,
        "no-this-assignment": false,
        "no-console": false,
        "ter-indent": 4,
        "variable-name": false,
        "ter-arrow-parens": false
    }
}
